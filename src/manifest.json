{
  "manifest_version": 2,
  "name": "Expel",
  "description": "This extension allows you to hide HTML elements",
  "version": "1.0",
  "browser_action": {
    "default_title": "Expel",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [  
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "js/main.js"
      ]
    }
  ]
}
